    "concurrently": "^6.0.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "parcel": "1.12.3",
    "react-test-renderer": "^17.0.2"
  },
  "jest": {
    "transform": {
      "\\.jsx": "babel-jest"
    }
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
